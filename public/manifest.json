{
  "name": "mercury",
  "description": "Chrome extension for mercury",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["webpage.js"]
    }
  ],
  "permissions": ["tabs"],
  "manifest_version": 2
}
